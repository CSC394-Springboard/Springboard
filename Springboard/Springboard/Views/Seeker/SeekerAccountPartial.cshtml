@using Springboard.Models
@model Springboard.Models.SeekerAccount
@if (Model != null)
{
    if (Model.CultureId == null)
    {
          @Html.Action("CultureCreatePartialSeekerAccount", "Culture", Model)
         
    }
    else
    {
        @Html.Action("CultureEditPartial", "Culture", Model.CultureId)
    }

    if (Model.SkillRequirementId == null)
    {
        @Html.Action("SkillCreatePartialSeekerAccount", "Skills", Model)
    }
    else
    {
        @Html.Action("SkillDetailsPartial", "Skills", Model.SkillRequirementId);
    }

    @Html.Action("SeekerApplication", "Application");
}