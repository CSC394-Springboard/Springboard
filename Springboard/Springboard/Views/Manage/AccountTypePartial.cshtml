@using Springboard.Models
@model Springboard.Models.ApplicationUser
@if (Model != null)
{
    if (Model.Role.Equals(Role.Seeker))
    {
        @Html.ActionLink("Dashboard", "Index", "Seeker")
    }
    else if (Model.Role.Equals(Role.Poster))
    {
        @Html.ActionLink("Dashboard", "Index", "Poster")
    }
}
